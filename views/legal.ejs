    <script>
      $(function () {
        $('.mysuc').hide()
        $('.mybtn').on('click', function () {
          $('.mysuc').fadeIn(1000)
        })

        var checkNum = $('#number').val()
        $('.myles').hide()

        if (checkNum == '1.1') {
          $('#myform').on('submit', function (e) {
            e.preventDefault()
            $('.myles').fadeOut(200)
            $('.myles').fadeIn(1000)
          })

          $('.courses-features ul li a').click(function (e) {
            if (this.href.slice(55) > '1.1') {
              $('.myles').fadeIn(1000)
              e.preventDefault()
            }
          })
        }
      })
