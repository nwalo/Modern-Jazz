    <!--====== Page Banner Start ======-->

    <section class="page-banner">
      <div
        class="page-banner-bg bg_cover"
        style="
          background-image: url(assets/images/bg/about.png);
          background-position: bottom;
        "
      >
        <div class="container">
          <div class="banner-content text-center">
            <h2 class="title">About Us</h2>
          </div>
        </div>
      </div>
    </section>

